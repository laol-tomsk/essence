<h2><%= @alpha_definition.name %> (Alpha Definition)</h2>
<p>Method: <%= link_to @alpha_definition.method_definition.name, @alpha_definition.method_definition %></p>

<div style="margin-top: 30px">
  <h3>States</h3>
  <ul>
    <% @alpha_definition.state_definitions.order(order: :asc).each do |state_definition| %>
      <li>
        <%= state_definition.name %>
      </li>
    <% end %>
  </ul>
</div>